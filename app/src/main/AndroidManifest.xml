<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/corona_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/corona_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.samsung.android.health.permission.read"
            android:value="com.samsung.health.heart_rate" />
        <activity android:name=".questionnaire.QuestionnaireActivity"/>
        <activity android:name=".questionnaire.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rrmeasure.RRMeasureActivity" />
        <activity android:name="com.example.covid.hrmeasure.HRMeasureActivity" />
        <activity android:name=".resultactivity.ResultActivity"/>
    </application>

</manifest>